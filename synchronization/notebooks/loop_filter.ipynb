      "source": [
        "def loop_filter(error_n, integral_n): # Gardner loop filter\n",
        "  kp = 0.01 \n",
        "  ki = 0.0001 \n",
        "  integral_n = error_n + integral_n\n",
        "  W = kp*error_n + ki*integral_n - 1\n",
        "  return (W, integral_n)"
      ]
    }
  ]
}