        "def modulate(vct_coded_bits):\n",
        "  vct_ak = np.array([])\n",
        "  for it in np.arange(0, len(vct_coded_bits)):\n",
        "    ak = complex(ALPHABET[vct_coded_bits[it]])\n",
        "    vct_ak = np.append(vct_ak, ak)\n",
        "  return vct_ak"
