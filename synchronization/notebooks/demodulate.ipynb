        "def demodulate(vct_detected):\n",
        "  vct_demod = np.array([])\n",
        "  for symbol in vct_detected:\n",
        "    bits_demod = INV_MAP[symbol]\n",
        "    vct_demod = np.append(vct_demod, bits_demod)\n",
        "  return vct_demod"
